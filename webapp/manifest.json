{
  "_version": "1.12.0",
  "sap.app": {
    "id": "org.ubb.books",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "booksService": {
        "uri": "/sap/opu/odata/SAP/Z801_LIBRARY_OTFI_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "org.ubb.books.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "org.ubb.books.i18n.i18n"
        }
      },
      "": {
        "dataSource": "booksService",
        "settings": {
          "metadataUrlParams": {
            "sap-documentation": "heading"
          },
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "org.ubb.books.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "appBookList",
          "target": "booklist"
        },
        {
          "pattern": "checked-out-books",
          "name": "appCheckedOutBooks",
          "target": "checkedOutBooks"
        },
        {
          "pattern": "client",
          "name": "appClientBooks",
          "target": "clientBooks"
        }
      ],
      "targets": {
        "booklist": {
          "viewId": "bookList",
          "viewName": "BookList",
          "viewLevel": 1
        },
        "checkedOutBooks": {
          "viewId": "checkedOutBooks",
          "viewName": "CheckedOutBooks",
          "viewLevel": 1
        },
        "clientBooks": {
          "viewId": "clientBooks",
          "viewName": "ClientBooks",
          "viewLevel": 1
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "show"
        }
      }
    }
  }
}
